<% provide(:title, "Current Students") %>

<div class="row">
	<div id="hub_text" class="col-sm-12">
		<div class="hub_header_image">
			<%= image_tag ('banners/students_b.jpg') %>
		</div>
		<h1> </h1>
		<h3 class="hub_para">For Graduate Students</h3>
	</div>

	

	<div id="hub_left" class="col-sm-6 clearfix">
		<ul>
			<li><%= link_to 'NGSA', '/students/ngsa', :class => 'ngsa_link' %></li>
			<li><%= link_to 'Support & Training Grants', '/prospect/finaid', :class => 'finaid_link' %></li>
			<li><%= link_to 'Resources', '/students/nsresources', :class => 'nsres_link' %></li>
		</ul>
	</div>
	<div id="hub_right" class="col-sm-6 clearfix">
		<ul>
			<li><%= link_to 'Outreach', '/students/outreach', :class => 'out_link' %></li>
			<li><%= link_to 'Distinguished Graduates', '/students/gmg', :class => 'gmgs_link' %></li>
		</ul>
	</div>

	<div id="hub_text" class="col-sm-12">
		<h1> </h1>
		<h3 class="hub_para">For Undergraduate Students</h3>
	</div>

	<p style="clear:both"></p>

	<div id="hub_left" class="col-sm-6 clearfix">
		<ul>
			<li><%= link_to 'NUSA', 'https://nolecentral.dsa.fsu.edu/organization/NUSA', :class => 'nusa_link', :target => "_blank" %></li>
		</ul>
	</div>
	<div id="hub_right" class="col-sm-6 clearfix">
		<ul>
			<li><%= link_to 'Tri-Beta', 'http://tri-beta.neuro.fsu.edu/', :class => 'trib_link', :target => "_blank" %></li>
		</ul>
	</div>
</div>






