<% provide(:title, "Admin Hub") %>

<div class="row">
	<div id="hub_text" class="col-sm-12">
		<div class="hub_header_image">
			<%= image_tag ('banners/resources_b.jpg') %>
		</div>
		<div class="filter btn btn-default btn-directory active">
	  <% if current_user.present? %>
        <%= link_to " Sign Out", sessions_path, method: :delete %>
      <% end %>
	</div>
		<h1> </h1>
	</div>

	<div id="hub_left" class="col-sm-6 clearfix">
		<ul>
			<li><%= link_to 'People', '/sekret/people/', :class => 'support_link' %></li>
		</ul>
	</div>
	<div id="hub_right" class="col-sm-6 clearfix">
		<ul>
			<li><%= link_to 'Tickets', '/sekret/tickets/', :class => 'cores_link' %></li>
		</ul>
	</div>
</div>
